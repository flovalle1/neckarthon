generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id     Int    @id @default(autoincrement())
    name   String
    number String
}

model IdeenMessage {
    id     Int    @id @default(autoincrement())
    text   String
    author String
}

model Meinungsbild {
    id    Int @id @default(autoincrement())
    one   Int
    two   Int
    three Int
}
